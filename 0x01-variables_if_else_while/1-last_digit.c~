#include <time.h>
#include <stdio.h>
#include <stdlib.h>
/**
* main - Entry point
*
* Return: Always 0 (Success)
*/
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
is = n % 10; 
if (is > 5)
{
  /** if condition is true then print the following */
  printf("Last digit of %i is %i and is greater than 5\n", is, n);
} else if (n < 6 && !=0)
{
  /** if condition is true then print the following */
  printf("Last digit of %i is %i and is less than 6 and not 0", is, n);
} else
{
  /** if condition is true then print the following */
  printf("Last digit of %i is %i and is 0 ", is, n);
}
return (0);
}
