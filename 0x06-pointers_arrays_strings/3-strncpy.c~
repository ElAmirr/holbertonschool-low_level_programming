#include "holberton.h"
/**
 * _strncp - concatenates two strings
 * @dest: coppy string
 * @src: append string
 * @n: nuber of bytes
 * Return: _strncp
 */
char *_strncpy(char *dest, char *src, int n)
{
int i = 0;
int j = 0;
while (dest[i] != '\0')
{
i++;
}
while (src[j] != '\0' && j<n)
{

dest[i] = src[j];
i++;
j++;

}
dest[i] = '\0';
return (dest);
}
